{
  "name": "naviware/salute",
  "description": "Minimal package for printing out greeting messages based on time of day",
  "license": "MIT",
  "keywords": [
    "Naviware",
    "Salute",
    "Greeting"
  ],
  "authors": [
    {
      "name": "Naviware",
      "email": "packagemanager@naviware.io",
      "homepage": "https://www.packages.naviware.io/salute/",
      "role": "Package Manager"
    }
  ],
  "require": {
    "php": ">=7.3",
    "nesbot/carbon": "^2.16"
  },
  "require-dev": {
    "phpunit/phpunit": ">=7",
    "mockery/mockery": "0.*",
    "symfony/var-dumper": "2.*"
  },
  "autoload": {
    "psr-4": {
      "Naviware\\Salute\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Naviware\\Salute\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
